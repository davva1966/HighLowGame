<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<div xmlns:c="http://java.sun.com/jsp/jstl/core" xmlns:field="urn:jsptagdir:/WEB-INF/tags/form/fields" xmlns:form="urn:jsptagdir:/WEB-INF/tags/form" xmlns:jsp="http://java.sun.com/JSP/Page" xmlns:spring="http://www.springframework.org/tags"
	xmlns:springform="http://www.springframework.org/tags/form" version="2.0">
	<jsp:directive.page contentType="text/html;charset=UTF-8" />
	<jsp:output omit-xml-declaration="yes" />
	<spring:message code="label_com_ss_speedsolutions_model_passwordchange" var="label" htmlEscape="false" />
	<form:create label="${label}" id="fc_com_ss_speedsolutions_model_PasswordChange" modelAttribute="changePassword" path="/changePassword">
		<field:input type="password" field="password" id="c_com_ss_speedsolutions_model_PasswordChange_currentPassword" required="true" max="100" />
		<field:input type="password" field="newPassword" id="c_com_ss_speedsolutions_model_PasswordChange_newPassword" required="true" max="100" />
		<field:input type="password" field="newPasswordConfirmation" id="c_com_ss_speedsolutions_model_PasswordChange_confirmNewPassword" required="true" max="100" />
	</form:create>
</div>
